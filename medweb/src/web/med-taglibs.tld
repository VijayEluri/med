      <name>searchResult</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>htmlID</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
